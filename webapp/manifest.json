{
    "_version": "1.65.0",
    "sap.app": {
        "id": "com.tushar.project2",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "0.0.1"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "sourceTemplate": {
            "id": "@sap/generator-fiori:basic",
            "version": "1.16.3",
            "toolsId": "20e7cda7-81e7-4e6c-83f4-2a4d841c6b12"
        },
        "dataSources": {
            "mainService": {
                "uri": "/V2/Northwind/Northwind.svc/",
                "type": "OData",
                "settings": {
                    "annotations": [],
                    "localUri": "localService/metadata.xml",
                    "odataVersion": "2.0"
                }
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": true,
        "dependencies": {
            "minUI5Version": "1.132.1",
            "libs": {
                "sap.m": {},
                "sap.ui.core": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "com.tushar.project2.i18n.i18n"
                }
            },
            "": {
                "dataSource": "mainService",
                "preload": true,
                "settings": {}
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "controlAggregation": "pages",
                "controlId": "app",
                "transition": "slide",
                "type": "View",
                "viewType": "XML",
                "path": "com.tushar.project2.view",
                "async": true,
                "viewPath": "com.tushar.project2.view"
            },
            "routes": [
                {
                    "name": "RouteTABLE",
                    "pattern": ":?query:",
                    "target": [ "TargetTABLE"]
                    
                },
                {
                    "name": "RouteViewProductdetail",
                    "pattern": "product/{pId}",   
                    "target": "ProductdetailTarget"      
                  }
            ],
            "targets": {
                "TargetTABLE": {
                    "id": "TABLE",
                    "name": "TABLE"
                },
                "ProductdetailTarget":  {
                    "id": "Productdetail",
                    "name": "Productdetail"
                }
            }
        },
        "rootView": {
            "viewName": "com.tushar.project2.view.App",
            "type": "XML",
            "async": true,
            "id": "App"
          }
    }
}